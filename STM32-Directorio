		  			 HAL_GPIO_WritePin(GPIOA, control1_Pin, 1);
		  			 HAL_GPIO_WritePin(GPIOA, control2_Pin, 0);

		  			 HAL_Delay(250);

		  			 data_out = binario_a_7(unidad);
		  			 GPIOB -> ODR = data_out;
		  			 HAL_GPIO_WritePin(GPIOA, control1_Pin, 0);
		  			 HAL_GPIO_WritePin(GPIOA, control2_Pin, 1);
		  			 HAL_Delay(300);
		  			 //FIN IMPRESION DIA

		  			 //FEEDBACK
		  			 data_out = 0x00;
		  			 GPIOB -> ODR = data_out;
		  			 HAL_GPIO_WritePin(GPIOB, GPIO_PIN_7, 1);
		  			 HAL_Delay(1000);
		  			 //FIN FEEDBACK

		  			 //INICIO IMPRESION MES
		  			 decena = AÑODEC_C2;
		  			 unidad = AÑOUNI_C2;

		  			 data_out = binario_a_7(decena);
		  			 GPIOB -> ODR = data_out;
		  			 HAL_GPIO_WritePin(GPIOA, control1_Pin, 1);
