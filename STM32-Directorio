						  for (i=0;i<=8;i++)
						  {
						  AÑOUNI_C1 = num[i];
						  }
					 }
					 else if ( set == 1 && UNIDEC == 1) //YA NO ES NECESARIO AQU�? DECENAS DE 6. EL MES NO LLEGA A 60
					 {
						  for (i=0;i<=8;i++)
						  {
						  AÑODEC_C1 = num[i];
						  }
					 }
				  break;

			  case 288:
					 if (set == 1 && UNIDEC ==0) //NUMERO 6 UNIDADES
					 {
						  for (i=0;i<=9;i++)
						  {
						  AÑOUNI_C1 = num[i];
						  }
					 }
					 else if ( set == 1 && UNIDEC == 1) //YA NO ES NECESARIO AQU�? DECENAS DE 6. EL MES NO LLEGA A 60
					 {
						  for (i=0;i<=9;i++)
						  {
						  AÑODEC_C1 = num[i];
						  }
					 }
				  break;


			  default:
					 if (set == 1 && UNIDEC ==0) //NUMERO 6 UNIDADES
					 {
						  for (i=0;i<=0;i++)
						  {
						  AÑOUNI_C1 = num[i];
						  }
					 }
					 else if ( set == 1 && UNIDEC == 1) //YA NO ES NECESARIO AQU�? DECENAS DE 6. EL MES NO LLEGA A 60
					 {
						  for (i=0;i<=0;i++)
						  {
						  AÑODEC_C1 = num[i];
						  }
					 }
				  break;
			  }

			  break; //FIN DEL BOTON  AÑO

		  default: // INICIO DEFAULT
			  break;
		  }
		  break;

	  case 512://**CONTACTO 1**//
		  switch (DMA)
		  		  {
		  		  case 2: //BOTON DE DIA
		  			  switch(numeros)
		  			  {
		  			  case 32: // NÚMERO 1
		  				     if (set == 1 && UNIDEC ==0) //NUMERO 1 UNIDADES
		  				     {
