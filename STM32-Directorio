		  						  {
		  						  MESDEC_C2 = num[i];
		  						  }
		  				     }
		  				  break;
		  			  }
		  			  break; //FIN DEL BOTON MES

		  		  case 8: // BOTÓN DE AÑO
		  			  switch(numeros)
		  			  {
		  			  case 32: // NÚMERO 1
		  					 if (set == 1 && UNIDEC ==0) //NUMERO 1 UNIDADES
		  					 {
		  						  for (i=0;i<=1;i++)
		  						  {
		  						  AÑOUNI_C2 = num[i];
		  						  }
		  					 }
		  					 else if ( set == 1 && UNIDEC == 1) //NUMERO 1 DECENAS
		  					 {
		  						  for (i=0;i<=1;i++)
		  						  {
		  						  AÑODEC_C2 = num[i];
		  						  }
		  					 }
		  					 break;

		  			  case 64:
		  					 if (set == 1 && UNIDEC ==0) //NUMERO 2 UNIDADES
		  					 {
		  						  for (i=0;i<=2;i++)
		  						  {
		  						  AÑOUNI_C2 = num[i];
		  						  }
		  					 }
		  					 else if ( set == 1 && UNIDEC == 1) //NUMERO 2 DECENAS
		  					 {
		  						  for (i=0;i<=2;i++)
		  						  {
