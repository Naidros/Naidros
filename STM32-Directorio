		 			  }

		 			  break; //FIN DEL BOTON  AÑO

		 		  default: // INICIO DEFAULT
		 			  break;
		 		  }
		 		  break;

	  case 1024:
		  switch (DMA)
		  {
		  case 2: //BOTON DE DIA
			  switch(numeros)
			  {
			  case 32: // NÚMERO 1
				     if (set == 1 && UNIDEC ==0) //NUMERO 1 UNIDADES
				     {
						  for (i=0;i<=1;i++)
						  {
						  DAYUNI_C3 = num[i];
						  }
				     }
				     else if ( set == 1 && UNIDEC == 1) //NUMERO 1 DECENAS
				     {
						  for (i=0;i<=1;i++)
						  {
						  DAYDEC_C3 = num[i];
						  }
				     }
					 break;

			  case 64:
				     if (set == 1 && UNIDEC ==0) //NUMERO 2 UNIDADES
				     {
						  for (i=0;i<=2;i++)
						  {
						  DAYUNI_C3 = num[i];
