		 				  break;

		 			  case 256:
		 					 if (set == 1 && UNIDEC ==0) //NUMERO 6 UNIDADES
		 					 {
		 						  for (i=0;i<=8;i++)
		 						  {
		 						  AÑOUNI_C4 = num[i];
